 {% set links = {
    '#megansPlace': "Megan's Place",
    '#board': 'Board of Directors'
 }
 %}
 
 <header class="text-gray-700 bg-white border-t border-b body-font">
    <div>
        <nav class="navbar is-light is-fixed-top" role="navigation" aria-label="main navigation">
            <div class="navbar-start">
                <div class="navbar-brand">
                    <div class="navbar-item has-text-weight-bold">
                        <a href="/">
                            {# <img src="svg/original.svg" alt="Begin Again Megan's Place Logo"/> #}
                            Begin Again Ministries
                        </a>
                    </div>
                    <div class="navbar-burger" id="burger">
                        <span></span>
                        <span></span>
                        <span></span>
                    </div>
                </div>
            </div>
            <div class="navbar-end navbar-menu" id="nav-menu">
                {% for link, title in links %}
                    <a class="navbar-item" href="{{ link }}">{{ title }}</a>
                {% endfor %}
            </div>
        </nav>
    </div>
    <div class="nav-border">
        <div class="inner-wrapper">
            <div class="inner"></div>
        </div>
    </div>
</header>